<div class="topnav bg-indigo-950">
	<a data-hx-boost="true" href="/">{{ appTitle }}</a>
	{{ range listPages "pages" "" }}
		{{ if not .Visible }}
			{{ continue }}
		{{ end }}
		<a data-hx-boost="true" href="{{ rootPath }}{{ .Path }}">{{ .Title }}</a>
	{{ end }}
	<!-- Stuff on the right side -->
	<div class="topnav-right h-6">
		<p class="bg-blue-200 rounded-full px-2 py-1">{{ getConfig.Version }}</p>
	</div>
</div>
